<style>
.team {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 120vh;
}

.member {
  position: relative;
  margin: 100px;
  overflow: hidden;
}

.member img {
  display: block;
  width: 400px;
  height: 1000px;
  object-fit: cover;
}

.overlay {
  position: absolute;
  bottom: -100%;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.7);
  color: #fff;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  transition: all 0.3s ease-in-out;
}

.overlay h3 {
  font-size: 24px;
  margin-bottom: 50px;
}

.overlay p {
  font-size: 18px;
}

.member:hover .overlay {
  bottom: 0;
}


</style>

<script>
    
    const members = document.querySelectorAll('.member');

    members.forEach(member => {
      const name = member.dataset.name;
      const position = member.dataset.position;
      
      const overlay = member.querySelector('.overlay');
      overlay.innerHTML = `
        <h3>${name}</h3>
        <p>${position}</p>
      `;
    });
    
</script>

